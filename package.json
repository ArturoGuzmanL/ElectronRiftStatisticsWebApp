{
  "name": "riftstatistics",
  "version": "1.0.0",
  "description": "RiftStatistics WebApp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "RiftStatistics",
    "productName": "RiftStatistics",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "../assets/icons/Windows.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "../assets/icons/Windows.ico",
      "uninstallerIcon": "../assets/icons/Windows.ico",
      "license": "LICENSE.txt",
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Arturo Guzm√°n Lucena",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "electron": "^24.1.0",
    "electron-builder": "^24.4.0",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "@sweetalert2/theme-bootstrap-4": "^5.0.15",
    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "crypto-js": "^4.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^5.1.0",
    "email-validator": "^2.0.4",
    "esm": "^3.2.25",
    "javascript": "^1.0.0",
    "jquery": "^3.6.4",
    "jsdom": "^21.1.1",
    "jwt-decode": "^3.1.2",
    "keytar": "^7.9.0",
    "mousetrap": "^1.6.5",
    "mousetraps": "^1.6.6",
    "mysql": "^2.18.1",
    "next": "^13.3.0",
    "next-auth": "^4.22.0",
    "react-cookie": "^4.1.1",
    "sweetalert2": "^11.4.8"
  }
}
